[user]
	user = troter
	email = trot.thunder@gmail.com

[color]
	ui = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[core]
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol

[alias]
	br = branch
	ci = commit
	co = checkout
	df = diff
	lg = log -p
	st = status
